<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="yourapp"> 		<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		  		<controller name="Typical"/>			</default-module>		<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadIssue">		<model name="Issue"/>		<controller name="Print"/>	</module>			-->		<module name="MyCalendar"> 		<model name="Issue"/>		<tab name="MyCalendar"/>		<controller name="MyCalendar"/>	</module>	</application>