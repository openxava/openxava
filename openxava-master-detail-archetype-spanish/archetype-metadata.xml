        <include>**/*.jrxml</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="ISO-8859-1">
      <directory>src/main/webapp</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="ISO-8859-1">
      <directory>src/main/resources</directory>
