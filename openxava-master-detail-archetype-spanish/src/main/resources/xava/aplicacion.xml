<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="tuaplicacion"> 		<!--	Un módulo por defecto se asume para cada	controlador en <modulo-defecto/> 	-->	<modulo-defecto>		  		<controlador nombre="Typical"/>			</modulo-defecto>		<!--	Podemos definir módulos adicionales, por ejemplo: 		<modulo nombre="ProfesorSoloLectura">		<modelo nombre="Profesor"/>		<controlador nombre="Print"/>	</modulo>			-->	  		<modulo nombre="CuadroMando">		<modelo nombre="CuadroMando"/>		<controlador nombre="InitModelWithNewInstance"/>	</modulo>		</aplicacion>